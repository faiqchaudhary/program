#include<stdio.h>
#include<stdlib.h>
#include<unistd.h>
{
  int array[1000];
  int pid[10];
  int sum[10];
  int totalsum;
  
    for(int i=0;i<1000;i++)
      {
        array[i]=i+1;
      }
      for(int i=0;i<10;i++)
      {
        sum[i]=0;
        pid[i]=fork();
        
        if(pid[i]==0)
        {
          for(int j=(i*100);j<(i*100)+100;j++)
          {
            sum[i]=sum[i]+array[j];
          }
            exit(0);
         }
         for(int i=0;i<10;i++)
         {
          wait(NULL);
         }
         for(int i=0;i<10;i++)
         {
          totalsum=totalsum+sum[i];
          }
          printf("TotalSum = %d",totalsum);
    }
